{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"ec2": {
			"dependsOn": ["start:dev", "start"],
			"cache": false,
			"persistent": true
		},
		"preview": {
			"cache": true,
			"persistent": false
		},
		"dev": {
			"dependsOn": ["start:nginx"],
			"persistent": true,
			"cache": false
		},
		"start:nginx": {
			"dependsOn": ["start:dev"],
			"cache": false,
			"persistent": false
		},
		"start": {
			"cache": false,
			"persistent": false
		},
		"start:dev": {
			"dependsOn": ["db:migrate:dev", "swagger:gen"],
			"cache": false,
			"persistent": false
		},
		"swagger:gen": {
			"cache": false
		},
		"db:migrate:dev": {
			"dependsOn": ["db:up"],
			"cache": false
		},
		"db:up": {
			"cache": false
		},
		"build": {
			"outputs": ["dist/**"]
		},
		"type-check": {}
	}
}
