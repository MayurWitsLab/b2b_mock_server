{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
    "dev": {
      "dependsOn": ["start", "start:dev"],
      "persistent": true,
      "cache": false
    },
    "start": {
			"cache": false,
			"persistent": false
    },
		"start:dev": {
			"dependsOn": ["db:migrate:dev", "swagger:gen"],
			"cache": false,
			"persistent": false
		},
		"swagger:gen": {
			"cache": false
		},
		"db:migrate:dev": {
			"dependsOn": ["db:up"],
			"cache": false
		},
		"db:up": {
			"cache": false
		},
		"build": {
			"outputs": ["dist/**"]
		},
		"type-check": {}
	}
}
